<div class="jumbotron text-center">
	<h1>Geek City</h1>

	<p>{{ tagline }}</p>
	<pre>'Coin:' {{btcPrice | json }}</pre>
	<div ng-repeat="user in beng">
		{{user.email}}
		{{user.username}}
		{{user._id}}
		<div class="btn btn-danger" ng-click="deleteUser(user)">Delete</div>
		<div class="btn btn-primary" ng-click="updateStudent(user)">Update</div>
	</div>
	<div class="btn btn-primary" ng-click="createStudent()">Create new</div>

</div>